#main
  position relative
  width 800px
  height 400px
  background-color #90ee90

  #character
    position absolute
    width 40px
    height 40px
    background-color black
    transition left 0.1s, bottom 0.1s

  .obstacle
    position absolute
    width 40px
    height 40px
    background-color green
    transition left 0.1s, bottom 0.1s